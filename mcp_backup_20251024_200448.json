      "type": "stdio",
      "command": "node",
      "args": [
        "/Users/a805120/.mcp-src/mcp-memory-keeper/dist/index.js"
      ],
      "env": {
        "DESCRIPTION": "Session/workspace continuity for coding (local clone)",
        "PURPOSE": "Stop re-explaining projects and losing state during long debugging.",
        "PACKAGE_NAME": "mcp-memory-keeper (local clone)",
        "AUTHOR": "mkreyman",
        "GITHUB_REPO": "https://github.com/mkreyman/mcp-memory-keeper",
        "TRANSPORT_TYPE": "stdio",
        "MCP_KEEPER_PROJECT_ROOT": "/Users/a805120/Projects",
        "MCP_KEEPER_STATE_DIR": "/Users/a805120/.mcp/keeper",
        "STATE_CONTENTS": "workspace list, recent files, tool toggles, lightweight prefs",
        "RETENTION_POLICY": "rotating snapshots under STATE_DIR",
        "SCOPE": "per-machine developer context",
        "SECURITY_NOTES": "local-only; may reveal repo paths; avoid storing secrets",
        "WORKFLOWS_INTEGRATED": "true",
        "TAGS": "developer,workspace,continuity"
      },
      "disabled": true
    },
