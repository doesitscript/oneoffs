{
  "mcpServers": {
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp"
      ],
      "env": {
        "HEADLESS": "false",
        "CHROME_BINARY": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome",
        "ALLOW_INSECURE_LOCALHOST": "true",
        "USER_DATA_DIR": "/Users/a805120/Library/Application Support/Google/Chrome/Default",
        "CHROME_DEBUG_PORT": "9222",
        "CHROME_DEBUG_HOST": "localhost"
      },
      "autoApprove": [
        "chrome_navigate",
        "chrome_get_web_content",
        "chrome_get_interactive_elements",
        "chrome_click_element",
        "chrome_fill_or_select",
        "chrome_keyboard",
        "chrome_wait_for",
        "chrome_screenshot"
      ]
    },
    "macos-automator": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@steipete/macos-automator-mcp@latest"
